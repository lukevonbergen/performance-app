{
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "https://venue.gostagelink.com",
      "statusCode": 301
    },
    {
      "source": "/performer-dashboard",
      "destination": "https://performer.gostagelink.com",
      "statusCode": 301
    }
  ],
  "rewrites": [
    {
      "source": "/:path",
      "destination": "/:path.html"
    },
    {
      "source": "/rate/venue/:venueId",
      "destination": "/rate.html"
    }
  ],
  "routes": [
    {
      "src": "^/.*$",
      "dest": "/index.html"
    }
  ]
}
